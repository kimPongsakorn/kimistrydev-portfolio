{
  "name": "with-nextjs-prisma",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn migrate:deploy && next build",
    "start": "next start --port ${PORT-3000}",
    "migrate:dev": "dotenv -e .env.development -- prisma migrate dev",
    "migrate:deploy": "dotenv -e .env.production -- prisma migrate deploy",
    "migrate:status": "dotenv -e .env.production -- prisma migrate status",
    "generate": "prisma generate"
  },
  "dependencies": {
    "@prisma/client": "4.9.0",
    "framer-motion": "^12.23.24",
    "hamburger-react": "^2.5.2",
    "next": "13.1.6",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "swr": "^2.0.3"
  },
  "devDependencies": {
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.27",
    "autoprefixer": "^10.4.22",
    "dotenv-cli": "^11.0.0",
    "postcss": "^8.5.6",
    "prisma": "4.9.0",
    "tailwindcss": "^3.4.18",
    "typescript": "^4.9.5"
  }
}
